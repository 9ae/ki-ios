  pod 'LayerXDK/UI'
  pod 'Cache'

  target 'iOSKinkedInTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'iOSKinkedInUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
